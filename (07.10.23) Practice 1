#include<stdio.h>
int main() {
        int num ;
        scanf("%d", &num) ;
        for (int i=0 ; i < num ; i++){
                for (int s=0 ; s <= num - i ; s++){
                        printf(" ");}
                for (int j=0 ; j <= i ; j++){
                        printf("* ");}
        printf("\n");}
}
