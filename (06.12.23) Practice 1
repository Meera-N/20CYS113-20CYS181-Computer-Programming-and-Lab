#include<stdio.h>
int main(){
	int a;
	int *b;
        int c=86;

	printf("enter value of a:\n");
	scanf("%d",&a);
       
	b=&a;

        //%x because memory is hexadecimal
	
        printf("a=%d \n&a=%d \nb=%x \n*b=%d\n",a,&a,b,*b);
        
        b=&c;

	printf("c=%d \n&c=%d \nb=%x \n*b=%d\n",c,&c,b,*b);}
	
