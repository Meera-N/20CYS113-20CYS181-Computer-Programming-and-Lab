#include<stdio.h>
int main(){

struct compNum{
   int inum;
   int rnum;
   } c[2];
   
   for(int i=0; i<2;i++){
     printf("Please enter real part:\n");
     scanf("%d",&c[i].rnum);
     printf("Please enter imaginary part:\n");
     scanf("%d",&c[i].inum);
   }
     
  int res1, res2;
   res1=c[0].inum*c[1].inum;
   res2=c[0].rnum*c[1].rnum;
  
    printf("The product is %d +%di", res2, res1);}
  
